#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ahmed/Documents/Work/kids-points/node_modules/.pnpm/shadcn-svelte@1.0.6/node_modules/shadcn-svelte/dist/node_modules:/Users/ahmed/Documents/Work/kids-points/node_modules/.pnpm/shadcn-svelte@1.0.6/node_modules/shadcn-svelte/node_modules:/Users/ahmed/Documents/Work/kids-points/node_modules/.pnpm/shadcn-svelte@1.0.6/node_modules:/Users/ahmed/Documents/Work/kids-points/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ahmed/Documents/Work/kids-points/node_modules/.pnpm/shadcn-svelte@1.0.6/node_modules/shadcn-svelte/dist/node_modules:/Users/ahmed/Documents/Work/kids-points/node_modules/.pnpm/shadcn-svelte@1.0.6/node_modules/shadcn-svelte/node_modules:/Users/ahmed/Documents/Work/kids-points/node_modules/.pnpm/shadcn-svelte@1.0.6/node_modules:/Users/ahmed/Documents/Work/kids-points/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/index.js" "$@"
else
  exec node  "$basedir/../../dist/index.js" "$@"
fi
